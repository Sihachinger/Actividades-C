// B2. Ingresar tres valores, imprimir la suma total, s√≥lo sabe sumar de a dos.

#include <stdio.h>

int main () {
    int num1, num2, num3, suma_total;
    printf("Ejercicio 2: Sumar tres valores\n");
    printf("Ingrese tres valores enteros: ");
    scanf("%d %d %d", &num1, &num2, &num3);
    suma_total = num1 + num2 + num3;
    printf("La suma total de %d, %d y %d es %d\n\n", num1, num2, num3, suma_total);
    return 0;
}
